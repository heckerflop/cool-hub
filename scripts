game:GetService("GuiService"):ClearError() 

local OrionLib = loadstring(game:HttpGet(("https://raw.githubusercontent.com/Giangplay/Script/main/Orion_Library_PE_V2.lua")))()

local Window = OrionLib:MakeWindow({Name = ("Cool Hub", HidePremium = false, IntroText = "Cool Hub", SaveConfig = true, ConfigFolder = "Scripts"})

-- Tabs
local Tab = Window:MakeTab({
	Name = "Universal",
	Icon = "rbxassetid://7743869612",
	PremiumOnly = false
})

-- Buttons
Tab:AddButton({
	Name = "Button!",
	Callback = function()
      		print("true")
  	end    
})
