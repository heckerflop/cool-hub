if game.PlaceId == 6403373529 then
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Cool Hub", HidePremium = false, IntroText = "Cool Hub", SaveConfig = true, ConfigFolder = "scripts"})

local Tab = Window:MakeTab({
	Name = "Universal",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab2:AddSlider({
	Name = "WalkSpeed",
	Min = 20,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
		Walkspeed = Value
	end    
})

Tab2:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 1000,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "JumpPower",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
		JumpPower = Value
	end    
})

Tab:AddButton({
	Name = "Dark Dex",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/heckerflop/cool-hub/main/dex"))()
  	end    
})

end
OrionLib:Init()
